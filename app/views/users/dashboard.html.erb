<body class="dashboard-body">
<div class="wrapper skill-icon-area-container">
    <div class="row">
        <div class="col-md-6">
            <h2 class="name">Welcome, <%= @user.name %></h2>
            <p>You have <strong><%= @user.minutes %> minutes</strong></p>
        </div>
        <div class="col-md-6">
            <div class="skills-logo-area">
                <img class="skills-icon" src="/assets/skills-icon.png">
                <p>Link to My skills</p>
            </div>
        </div>
    </div>
</div>
<h1 class="dashboard">Dashboard</h1>


<div class="container">
<h1>Dashboard</h1>
<p>Paste the rest here!</p>
<p><%= link_to "Sign Out", destroy_user_session_path, method: 'DELETE' %></p>
<p><%= link_to "My stats", user_path(current_user) %></p>
</div>
</body>